proxy-groups:
  - name: 🇯🇵 日本手动
    type: select
    proxies:
      - 🇯🇵 B20 日本*
      - 🇯🇵 B21 日本*
      - 🇯🇵 B22 日本*
      - 🇯🇵 D21 日本*
      - 🇯🇵 D22 日本*
      - 🇯🇵 D23 日本*
      - 🇯🇵 D24 日本*
      - 🇯🇵 D25 日本*
  - name: 🇺🇲 美国手动
    type: select
    proxies:
      - 🇺🇲 B40 美国*
      - 🇺🇲 D40 美国*
      - 🇺🇲 D41 美国*
      - 🇺🇲 D42 美国*
      - 🇺🇲 D43 美国*
  - name: 💰 Salesfoce
    type: select
    proxies:
      - 🚀 节点选择
      - 🇭🇰 香港节点
      - 🇯🇵 日本节点
      - 🇯🇵 日本手动
      - 🇺🇲 美国节点
      - 🇺🇲 美国手动
      - 🚀 手动切换
      - DIRECT
